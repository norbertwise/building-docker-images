pipeline{

	agent {
        docker{
            image 'node:19-alpine'
        }
	}
	stages {
	    
	    stage('checkout') {

			steps {
				git branch : 'main' , url : 'https://github.com/norbertwise/building-docker-images.git'
			}
		}

		stage(' docker build') {
			steps{
				scripts{
					sh 'docker build -t dockweb:v1.12 .'
				}
			}
		}

			
			
				
			
		}

	}

