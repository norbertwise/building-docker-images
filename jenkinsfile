pipeline{

	agent {
        docker{
            image 'node:19-alpine'
        }
	}
	stages {
	    
	    stage('checkout') {

			steps {
				checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/norbertwise/building-docker-images.git']])
			sh 'npm install'
			
			}
		}

		
						
						
				}
			}
		

			
			
				
			
		

	

