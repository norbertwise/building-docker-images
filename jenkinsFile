pipeline{
    agent{
        docker{
            image 'node:19-alpine'
        }
    }
stages{
    stage('build'){
        steps{
        checkout scm

    docker.withRegistry('https://registry.hub.docker.com', 'dockerbuild') {

        def customImage = docker.build("patnorbs/web-app")

        /* Push the container to the custom Registry */
        customImage.push()
    
        }
    }
}


}
}
